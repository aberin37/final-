// A $( document ).ready() block.
$(document).ready(function(){
    $('.parallax').parallax();
  });

$( document ).ready(function() {
    console.log( "ready!" );

    // when i click to find out
    //reveal more information

    $("a").on("click", function() {
      $(".answer").append("<h1>80%.</h1>");
    });
});

 
